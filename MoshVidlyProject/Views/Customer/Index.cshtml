@model MoshVidlyProject.ViewModel.RandomMovieViewModel

@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Movie.Name</h2>

@if (Model.Customers.Count == 0)
{
    <text>No more Has rented this movie before.</text>
}
else
{

    foreach (var item in Model.Customers)
    {
        <li>@Html.ActionLink("customer/details", "Details", "Customer")@item.Name</li>
    }
}
