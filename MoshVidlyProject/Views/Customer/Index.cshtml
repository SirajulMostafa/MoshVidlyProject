    
@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model.Count == 0)
{
    <text>No more Has rented this movie before.</text>
}
else
{

    foreach (var customer in Model)
    {
        <li>@Html.ActionLink("customer/details/", "Details", "Customer")@customer.Name
    </li>
    }
}
